{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "http://localhost:5000",
      "https://localhost:5001",
      "https://localhost:5002",
      "http://localhost:5003",
      "http://localhost:8080",
      "https://localhost:8080",
      "http://localhost:31653",
      "https://localhost:44363"
    ],
    "ExposedHeaders": [
      "X-Token-Expired",
      "X-Correlation-Id",
      "X-Rate-Limit-Limit",
      "X-Rate-Limit-Remaining",
      "X-Rate-Limit-Reset"
    ]
  },
  "Swagger": {
    "Enabled": true,
    "SupportedApiVersions": ["v1", "v2"],
    "AuthSettings": {
      "RequireAuth": false
    }
  }
}
